    "#Credetianls for Accessing AWS ans also to identify our permessions\n",
    "access_key = \""\n",
    "secret_key =\"\""
   ]
  },
  {
   "cell_type": "markdown",
   "id": "ec549160-a23b-43af-9692-585448fc1c70",
   "metadata": {},
   "source": [
    "# Connecting with Bedrock"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "350934f7-03f5-4f3b-aaa7-24abfe03fa52",
   "metadata": {},
   "outputs": [],
   "source": [
    "import boto3\n",
    "import json\n",
    "from botocore.config import Config\n",
    "\n",
    "aws_access_key = access_key\n",
    "aws_secret_key = secret_key\n",
    "aws_region = \"us-east-1\"\n",
    "\n",
    "config = Config(read_timeout=3600)\n",
    "\n",
    "bedrock = boto3.client(\n",
    "    \"bedrock-runtime\",\n",
    "    aws_access_key_id=aws_access_key,\n",
    "    aws_secret_access_key=aws_secret_key,\n",
    "    region_name=aws_region,\n",
    "    config=config\n",
    ")\n",
    "\n",
    "messages = []\n",
    "\n",
    "while True:\n",
    "    prompt = input(\"Please ask your question: \")\n",
    "    messages.append({\n",
    "        \"role\": \"user\",\n",
    "        \"content\": [{\"text\": prompt}]\n",
    "    })\n",
    "\n",
    "    body = {\n",
    "        \"messages\": messages,\n",
    "        \"inferenceConfig\": {\n",
    "            \"maxTokens\": 10000,\n",
    "            \"temperature\": 1,\n",
    "            \"topP\": 1\n",
    "        }\n",
    "    }\n",
    "\n",
    "    try:\n",
    "        response = bedrock.invoke_model(\n",
    "            modelId=\"us.amazon.nova-premier-v1:0\",  # Valid inference profile ID\n",
    "            body=json.dumps(body),\n",
    "            contentType=\"application/json\",\n",
    "            accept=\"application/json\"\n",
    "        )\n",
    "        response_body = json.loads(response[\"body\"].read())\n",
    "        output_msg = response_body[\"output\"][\"message\"][\"content\"][0][\"text\"]\n",
    "\n",
    "        print(\"\\nModel Output:\\n\")\n",
    "        print(output_msg, \"\\n\")\n",
    "\n",
    "        messages.append({\n",
    "            \"role\": \"assistant\",\n",
    "            \"content\": [{\"text\": output_msg}]\n",
    "        })\n",
    "    except Exception as e:\n",
    "        print(f\"Error: {e}\")"
   ]
  },
