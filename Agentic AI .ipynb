    "# Using Calculator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "47093122-73bf-4702-8c55-58d317da4525",
   "metadata": {},
   "outputs": [],
   "source": [
    "from strands import Agent, tool\n",
    "from strands_tools import calculator\n",
    "\n",
    "\n",
    "\n",
    "# Create an agent with tools\n",
    "agent_with_tools = Agent(callback_handler=None,\n",
    "    model=\"us.amazon.nova-premier-v1:0\",\n",
    "    tools=[calculator],\n",
    "    system_prompt=\"You are a helpful assistant. You can do math calculations.\"\n",
    ")\n",
    "\n",
    "# Test the agent with both tools in one query\n",
    "# Note: This query requires both tools - the weather tool to get temperature in Celsius,\n",
    "# and the calculator tool to convert from Celsius to Fahrenheit.\n",
    "# The agent will automatically determine which tools to use and in what order.\n",
    "response = agent_with_tools(input(\"What is the your question\"))\n",
    "print(response)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ce5b09c0-3a71-43e6-bd04-78d0701ca28a",
   "metadata": {},
   "outputs": [],
   "source": [
    "result = agent_with_tools.tool.calculator(expression=\"2 + 3 * 4\")\n",
    "print(f\"Calculator result: {result}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "122df908-4ece-424b-b5d4-be33896eac15",
   "metadata": {},
   "source": [
    "## Search on WEB "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b3572993-d345-4e3b-8ab4-230afce0a18b",
   "metadata": {},
   "outputs": [],
   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "from strands import tool\n",
    "\n",
    "@tool\n",
    "def fetch_site(url: str, selector: str) -> str:\n",
    "    \"\"\"\n",
    "    Fetches text content from a specific website using a CSS selector.\n",
    "    \"\"\"\n",
    "    headers = {\"User-Agent\": \"Mozilla/5.0\"}\n",
    "    r = requests.get(url, headers=headers, timeout=10)\n",
    "\n",
    "    if r.status_code != 200:\n",
    "        return \"Failed to fetch page\"\n",
    "\n",
    "    soup = BeautifulSoup(r.text, \"html.parser\")\n",
    "    el = soup.select_one(selector)\n",
    "\n",
    "    return el.get_text(strip=True) if el else \"Element not found\"\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9541d17b-8366-44fd-9041-b9b434fc7619",
   "metadata": {},
   "outputs": [],
   "source": [
    "agent = Agent(\n",
    "    model=\"us.amazon.nova-premier-v1:0\",\n",
    "    tools=[fetch_site],\n",
    "    system_prompt=\"\"\"\n",
    "You can fetch data from trusted websites.\n",
    "Use fetch_site only when explicitly needed.\n",
    "\"\"\"\n",
    ")\n"
   ]
  },
